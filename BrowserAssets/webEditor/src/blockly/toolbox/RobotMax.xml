<category name="运动" colour="#d061bc" icon="action">
    <label text="运动"></label>
    <block type="setMotionMode"></block>
    <block type="faceToPosition">
        <value name="x">
            <shadow type="math_number">
                <field name="NUM">100</field>
            </shadow>
        </value>
        <value name="z">
            <shadow type="math_number">
                <field name="NUM">100</field>
            </shadow>
        </value>
    </block>
    <block type="setRotation">
        <value name="angle">
            <shadow type="singleAngle">
                <field name="angle">90</field>
            </shadow>
        </value>
    </block>
    <block type="rotateUntilEnd">
        <value name="angle">
            <shadow type="singleAngle">
                <field name="angle">15</field>
            </shadow>
        </value>
    </block>
    <block type="setRateInFoot">
        <value name="rate">
            <shadow type="rateValueInFoot">
                <field name="NUM">0.3</field>
            </shadow>
        </value>
    </block>
    <block type="setRateAndTimeInFoot">
        <value name="rate">
            <shadow type="rateValueInFoot">
                <field name="NUM">0.3</field>
            </shadow>
        </value>
        <value name="time">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
    </block>
    <block type="jumpInFoot">
        <value name="distance">
            <shadow type="distanceValueInFoot">
                <field name="NUM">20</field>
            </shadow>
        </value>
        <value name="height">
            <shadow type="jumpValueInFoot">
                <field name="NUM">20</field>
            </shadow>
        </value>
    </block>
    <block type="backflipInFoot">
        <value name="distance">
            <shadow type="distanceValueInFoot">
                <field name="NUM">20</field>
            </shadow>
        </value>
        <value name="height">
            <shadow type="backflipValueInFoot">
                <field name="NUM">20</field>
            </shadow>
        </value>
    </block>
    <block type="setRateInWheel">
        <value name="rate">
            <shadow type="rateValueInWheel">
                <field name="NUM">0.3</field>
            </shadow>
        </value>
    </block>
    <block type="setRateAndTimeInWheel">
        <value name="rate">
            <shadow type="rateValueInWheel">
                <field name="NUM">0.3</field>
            </shadow>
        </value>
        <value name="time">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
    </block>
    <block type="stopMotion" />
    <block type="getDirection" />
</category>
<category name="感知" colour="#ff6464" icon="detection">
    <label text="感知"></label>    
    <block type="enableInfraredDepthSensor" />
    <block type="getInfraredDepthSensorValue" />
    <block type="whenInfraredDepthSensorValue">
        <value name="value">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
    </block>
    <block type="enableTerrainAwareness" />
    <block type="getTerrainAwarenessValue" />
    <block type="checkTerrainValue" />
    <!-- <block type="getLongitudeOrLatitude" /> -->
    <block type="getPosition" />
    <block type="checkIfPassCoordinate">
        <value name="x">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
        <value name="y">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
    </block>
    <block type="getCountTime" />
    <block type="resetCountTime" />
    <block type="takePhoto" />
    <block type="recordVideo" />
</category>
<category name="智能" colour="#3eaedd" icon="wifi">
