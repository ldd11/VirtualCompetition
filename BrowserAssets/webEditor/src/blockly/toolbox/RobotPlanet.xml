<xml>
  <category name="事件" colour="#3bd6ff" icon="microbit">
    <label text="事件"></label>
    <block type="funcMain" />
    <block type="onRecvBroadcast">
      <value name="msg">
        <shadow type="text">
          <field name="TEXT">Hi</field>
        </shadow>
      </value>
    </block>
    <block type="sendBroadcast">
      <value name="msg">
        <shadow type="text">
          <field name="TEXT">Hi</field>
        </shadow>
      </value>
    </block>
    <!-- <block type="print">
      <value name="msg">
        <shadow type="text">
          <field name="TEXT">Hi</field>
        </shadow>
      </value>
    </block> -->
  </category>
  <category name="控制" colour="#a568f0" icon="control">
    <label text="控制"></label>
    <block type="repeatDo"></block>
    <block type="repeatDoTimes">
      <value name="times">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="controlBreak"></block>
    <block type="controlIf"></block>
    <block type="controls_if_no_else"></block>
    <block type="wait">
      <value name="time">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="waitUntil">
      <value name="condition"></value>
    </block>
    <block type="repeatDoAndWaitUntil">
      <value name="condition"></value>
    </block>
  </category>
  <category name="运算" colour="#f3ab3a" icon="math">
    <label text="运算"></label>
    <block type="math_number"></block>
    <block type="math_arithmetic">
      <value name="A">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="B">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="randomInt">
      <value name="startNum">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="endNum">
        <shadow type="math_number">
          <field name="NUM">5</field>
        </shadow>
      </value>
    </block>
    <block type="sinCos">
      <value name="degree">
        <shadow type="math_number">
          <field name="NUM">45</field>
        </shadow>
      </value>
    </block>
    <block type="mathRound">
      <value name="value">
        <shadow type="math_number">
          <field name="NUM">1.1</field>
        </shadow>
      </value>
    </block>
    <block type="math_advanced">
      <value name="number">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="takeReminder">
      <value name="dividend">
        <shadow type="math_number">
          <field name="NUM">27</field>
        </shadow>
      </value>
      <value name="divisor">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="math_constrain">
      <value name="LOW">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="HIGH">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
    <block type="mapNumber">
      <value name="ks">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="ke">
        <shadow type="math_number">
          <field name="NUM">-1</field>
        </shadow>
      </value>
      <value name="vs">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="ve">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="key">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
    <block type="math_pick">
      <value name="FIRST">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="SECOND">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="math_between">
      <value name="SRC">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="LIMIT_1">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="LIMIT_2">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="logic_compare">
      <field name="OP">EQ</field>
      <value name="A">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
      <value name="B">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="lengthV2">
      <value name="x1">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
      <value name="y1">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
      <value name="x2">
        <shadow type="math_number">
          <field name="NUM">200</field>
        </shadow>
      </value>
      <value name="y2">
        <shadow type="math_number">
          <field name="NUM">200</field>
        </shadow>
      </value>
    </block>
    <sep gap="40"></sep>
    <block type="transfer_to_string"></block>
    <block type="transfer_to_number"></block>
    <sep gap="40"></sep>
    <block type="text"></block>
    <block type="strJoin">
      <value name="str1">
        <shadow type="text">
          <field name="TEXT">a</field>
        </shadow>
      </value>
      <value name="str2">
        <shadow type="text">
          <field name="TEXT">b</field>
        </shadow>
      </value>
    </block>
    <block type="strLen">
      <value name="str">
        <shadow type="text">
          <field name="TEXT">b</field>
        </shadow>
      </value>
    </block>
    <block type="strSubstr">
      <value name="str">
        <shadow type="text">
          <field name="TEXT">hello</field>
        </shadow>
      </value>
      <value name="start">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="count">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="string_compare">
      <value name="src">
        <shadow type="text">
          <field name="TEXT">abc</field>
        </shadow>
      </value>
      <value name="dest">
        <shadow type="text">
          <field name="TEXT">ab</field>
        </shadow>
      </value>
    </block>
    <sep gap="40"></sep>
    <block type="logic_negate" />
    <block type="logic_operation" />
  </category>
  <category name="变量" colour="#ff7f46" icon="variables">
    <label text="变量"></label>
    <button text="创建变量" callbackKey="buttonPressedCreateGlobalVar"></button>
    <block type="variablesGet"></block>
    <block type="variablesSet">
      <value name="value">
        <shadow type="easy_text_number">
          <field name="text_number">0</field>
        </shadow>
      </value>
    </block>
    <sep gap="40"></sep>
    <button text="创建列表" callbackKey="buttonPressedCreateGlobalList"></button>
    <block type="listsGet"></block>
    <block type="listsAppend">
      <value name="value">
        <shadow type="easy_text_number">
          <field name="text_number">东西</field>
        </shadow>
      </value>
      <value name="lists">
        <block type="listsGet"></block>
      </value>
    </block>
    <block type="listsRemoveNthElement">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
      <value name="nth">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="listsRemoveAll">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
    </block>
    <block type="listsInsertElementBeforeNthElement">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
      <value name="nth">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="value">
        <shadow type="easy_text_number">
          <field name="text_number">1</field>
        </shadow>
      </value>
    </block>
    <block type="listsReplaceNthElement">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
      <value name="nth">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="value">
        <shadow type="easy_text_number">
          <field name="text_number">1</field>
        </shadow>
      </value>
    </block>
    <block type="listsNthElement">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
      <value name="nth">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="listsFirstPosOfElementAppears">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
      <value name="value">
        <shadow type="easy_text_number">
          <field name="text_number">东西</field>
        </shadow>
      </value>
    </block>
    <block type="listsElementsNum">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
    </block>
    <block type="listsContainsElement">
      <value name="lists">
        <block type="listsGet"></block>
      </value>
      <value name="value">
        <shadow type="easy_text_number">
          <field name="text_number">东西</field>
        </shadow>
      </value>
    </block>
  </category>
  <category name="函数" custom="PROCEDURE" colour="#30ffd9" icon="functions">
    <label text="函数"></label>
  </category>
    <category name="行星运动" colour="#d061bc" icon="action">
        <label text="行星运动"></label>
        <block type="configPlanetAction">
            <field name="planet">3</field>
            <field name="orbit">4</field>
            <value name="revolution_period">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
                <shadow type="planetPeriod">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="rotation_period">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
                <shadow type="planetPeriod">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="setSolarSystemTimeRatio">
            <field name="time_ratio">1</field>
        </block>
    </category>
</xml>